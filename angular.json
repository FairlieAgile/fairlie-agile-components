{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "demo",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "demo": {
      "projectType": "application",
      "root": "apps/demo",
      "sourceRoot": "apps/demo/src",
      "prefix": "fairlie-agile-components",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/demo",
            "index": "apps/demo/src/index.html",
            "main": "apps/demo/src/main.ts",
            "polyfills": "apps/demo/src/polyfills.ts",
            "tsConfig": "apps/demo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/demo/src/favicon.ico", "apps/demo/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/demo/src/environments/environment.ts",
                  "with": "apps/demo/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "stage": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/demo/src/environments/environment.ts",
                  "with": "apps/demo/src/environments/environment.stage.ts"
                }
              ],
              "outputHashing": "all"
            },
            "tenant3": {
              "fileReplacements": [
                {
                  "replace": "apps/demo/src/environments/environment.ts",
                  "with": "apps/demo/src/environments/environment.tenant3.ts"
                }
              ],
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            },
            "stage": {
              "browserTarget": "demo:build:stage"
            },
            "tenant3": {
              "browserTarget": "demo:build:tenant3"
            },
            "development": {
              "browserTarget": "demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/demo/src/**/*.ts",
              "apps/demo/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/demo"],
          "options": {
            "jestConfig": "apps/demo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-e2e": {
      "root": "apps/demo-e2e",
      "sourceRoot": "apps/demo-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo-e2e/cypress.json",
            "tsConfig": "apps/demo-e2e/tsconfig.e2e.json",
            "devServerTarget": "demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/demo-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ui-mat-incrementor": {
      "projectType": "library",
      "root": "libs/ui/mat-incrementor",
      "sourceRoot": "libs/ui/mat-incrementor/src",
      "prefix": "fa",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "project": "libs/ui/mat-incrementor/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ui/mat-incrementor/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/ui/mat-incrementor/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/mat-incrementor"],
          "options": {
            "jestConfig": "libs/ui/mat-incrementor/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/mat-incrementor/src/**/*.ts",
              "libs/ui/mat-incrementor/src/**/*.html"
            ]
          }
        }
      }
    },
    "ui-material": {
      "projectType": "library",
      "root": "libs/ui/material",
      "sourceRoot": "libs/ui/material/src",
      "prefix": "fa",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/material"],
          "options": {
            "jestConfig": "libs/ui/material/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/material/src/**/*.ts",
              "libs/ui/material/src/**/*.html"
            ]
          }
        }
      }
    },
    "ui-dev-banner": {
      "projectType": "library",
      "root": "libs/ui/dev-banner",
      "sourceRoot": "libs/ui/dev-banner/src",
      "prefix": "fa",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "project": "libs/ui/dev-banner/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ui/dev-banner/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/ui/dev-banner/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/dev-banner"],
          "options": {
            "jestConfig": "libs/ui/dev-banner/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/dev-banner/src/**/*.ts",
              "libs/ui/dev-banner/src/**/*.html"
            ]
          }
        }
      }
    },
    "util-fa-tenant": {
      "projectType": "library",
      "root": "libs/util/fa-tenant",
      "sourceRoot": "libs/util/fa-tenant/src",
      "prefix": "fa-tenant",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "project": "libs/util/fa-tenant/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/util/fa-tenant/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/util/fa-tenant/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/util/fa-tenant"],
          "options": {
            "jestConfig": "libs/util/fa-tenant/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/util/fa-tenant/src/**/*.ts",
              "libs/util/fa-tenant/src/**/*.html"
            ]
          }
        }
      }
    }
  }
}
